{
	"env": {
		"browser": true,
		"es2021": true
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 13,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"extends": ["next", "prettier"],
	"rules": {
		"react-hooks/exhaustive-deps": "off",
		"react/no-unescaped-entities": "off",
		"react/display-name": "off",
		"@next/next/no-img-element": "off"
	}
}
