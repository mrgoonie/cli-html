{
	"name": "cli-html",
	"version": "3.0.6",
	"description": "There will be cli module who render HTML to Terminal",
	"scripts": {
		"build": "rm -rf dist/** && tsc -p tsconfig.json && tsc-alias -p tsconfig.json chmod -R +x bin",
		"fix": "eslint --fix index.js lib/*.js lib/*/*.js test/*.js",
		"test": "yarn run html examples/demo.html"
	},
	"bin": {
		"html": "./dist/bin.js"
	},
	"main": "dist/index.js",
	"author": "Grigorii Horos",
	"license": "GPL3",
	"keywords": [
		"html",
		"render",
		"terminal",
		"cli",
		"html-to-cli",
		"html-to-terminal"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/horosgrisa/cli-html.git"
	},
	"engines": {
		"node": ">=16",
		"npm": ">=8"
	},
	"homepage": "https://github.com/horosgrisa/cli-html",
	"dependencies": {
		"@types/node": "^18.11.9",
		"ansi-align": "^3.0.1",
		"ansi-escapes": "^6.0.0",
		"boxen": "^7.0.0",
		"chalk": "^5.1.2",
		"change-case": "^4.1.2",
		"cli-highlight": "^2.1.11",
		"cli-table3": "^0.6.3",
		"color-namer": "^1.4.0",
		"compose-function": "^3.0.3",
		"concat-stream": "^2.0.0",
