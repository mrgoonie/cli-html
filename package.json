		"eslint-plugin-jsdoc": "^39.3.23",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-no-loops": "^0.3.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prefer-object-spread": "^1.2.1",
		"eslint-plugin-simple-import-sort": "^8.0.0",
